{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "project.pug",
          "project.js",
          "style.css",
          "osm-routing.png",
          "documentation.pdf",
          "lib/**",
          "data/**"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "server.js" }
  ]
}